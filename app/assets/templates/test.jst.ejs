<% for(i = 0; i < statuses.length; i++){ %>
    	
    	<div class='bubble'><h3 class="retweet"> <%= statuses[i].retweet_count%> retweets!</h3><h2 class='name'> <%= statuses[i].user.screen_name %>
    		(<span class="followers"><%= statuses[i].user.followers_count %></span> followers) said:</h2>
    		<span class="text"><%= statuses[i].text %></span></div>   
<%  } %>